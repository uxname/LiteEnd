{
  "name": "liteend",
  "version": "0.0.1",
  "description": "Lightweight, fast, and easy to use backend app template for Node.js",
  "author": "uxname",
  "private": true,
  "license": "MIT",
  "scripts": {
    "________________ BUILD AND RUN ________________": "",
    "save-commit-info": "npx ts-node src/common/git-commit-saver.ts",
    "build": "rimraf dist && npm run save-commit-info && npx run-p lint build:nest",
    "build:nest": "nest build",
    "start:dev": "npx cross-env NODE_ENV=development nest start --watch",
    "start:debug": "npx cross-env NODE_ENV=development nest start --debug --watch",
    "prestart:prod": "npm run db:migrations:apply",
    "start:prod": "npx cross-env NODE_ENV=production node --enable-source-maps dist/src/main",
    "________________ FORMAT AND LINT ________________": "",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "check": "npx run-p ts:check lint",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "ts:check": "tsc --noEmit",
    "________________ TEST ________________": "",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "________________ DATABASE ________________": "",
    "db:push": "npx prisma db push && npm run db:gen",
    "db:reset": "npx prisma migrate reset --force && npm run db:push && npm run db:gen",
    "db:migrations:apply": "npx prisma migrate deploy && npm run db:gen",
    "db:migrations:create": "npx prisma migrate dev --create-only",
    "db:schema:format": "npx prisma format",
    "db:gen": "npx prisma generate --no-hints",
    "db:studio": "npx prisma studio --browser none",
    "db:studio-local": "npm run db:studio -- -n 127.0.0.1",
    "db:seed": "ts-node src/seed.ts",
    "________________ OTHER ________________": "",
    "postinstall": "npm run prepare",
    "update": "npx npm-check-updates -u && npx rimraf node_modules package-lock.json && npm i --legacy-peer-deps",
    "postupdate": "npm run check && npm run lint:fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@apollo/gateway": "^2.9.3",
    "@apollo/server": "^4.11.2",
    "@bull-board/api": "^6.5.3",
    "@bull-board/express": "^6.5.3",
    "@bull-board/nestjs": "^6.5.3",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/apollo": "^12.2.2",
    "@nestjs/bull": "^10.2.3",
    "@nestjs/common": "^10.4.13",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.13",
    "@nestjs/graphql": "^12.2.2",
    "@nestjs/platform-express": "^10.4.13",
    "@nestjs/platform-socket.io": "^10.4.13",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/websockets": "^10.4.13",
    "@prisma/client": "^6.0.1",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "bull": "^4.16.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "graphql": "^16.9.0",
    "graphql-query-complexity": "^1.0.0",
    "graphql-type-json": "^0.3.2",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "log4js": "^6.9.1",
    "log4js-json-layout": "^2.2.3",
    "nestjs-i18n": "^10.5.0",
    "nodemailer": "^6.9.16",
    "otplib": "^12.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "ts-morph": "^24.0.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@jetbrains/eslint-config": "^6.0.4",
    "@nestjs/cli": "^10.4.8",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.13",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/jest": "29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.1",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-local": "^1.0.38",
    "@types/serve-index": "^1.9.4",
    "@types/source-map-support": "^0.5.10",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-comments": "^1.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-no-secrets": "^2.1.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "jest": "29.7.0",
    "npm-check-updates": "^17.1.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "prisma": "^6.0.1",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.2",
    "typescript-eslint": "^8.17.0"
  }
}
