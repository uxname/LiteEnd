{
  "name": "liteend",
  "version": "0.0.1",
  "description": "Lightweight, fast, and easy to use backend app template for Node.js",
  "author": "uxname",
  "private": true,
  "license": "MIT",
  "scripts": {
    "________________ BUILD AND RUN ________________": "",
    "save-commit-info": "npx ts-node src/common/git-commit-saver.ts",
    "prebuild": "rimraf dist && npm run lint && npm run save-commit-info",
    "build": "nest build",
    "start:dev": "npx cross-env NODE_ENV=development nest start --watch",
    "start:debug": "npx cross-env NODE_ENV=development nest start --debug --watch",
    "prestart:prod": "npm run db:migrations:apply",
    "start:prod": "npx cross-env NODE_ENV=production node --enable-source-maps dist/src/main",
    "________________ FORMAT AND LINT ________________": "",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "check": "npm run ts:check && npm run lint",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "ts:check": "tsc --noEmit",
    "________________ TEST ________________": "",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "________________ DATABASE ________________": "",
    "db:push": "npx prisma db push && npm run db:gen",
    "db:reset": "npx prisma migrate reset --force && npm run db:push && npm run db:gen",
    "db:migrations:apply": "npx prisma migrate deploy && npm run db:gen",
    "db:migrations:create": "npx prisma migrate dev --create-only",
    "db:schema:format": "npx prisma format",
    "db:gen": "npx rimraf src/@generated/nestgraphql && npx prisma generate",
    "db:studio": "npx prisma studio --browser none",
    "db:studio-local": "npm run db:studio -- -n 127.0.0.1",
    "db:seed": "ts-node src/seed.ts",
    "________________ OTHER ________________": "",
    "postinstall": "npm run prepare",
    "update": "npx npm-check-updates -u && npx rimraf node_modules package-lock.json && npm i --legacy-peer-deps",
    "postupdate": "npm run check && npm run lint:fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@apollo/gateway": "^2.8.0",
    "@apollo/server": "^4.10.4",
    "@bull-board/api": "^5.20.1",
    "@bull-board/express": "^5.20.1",
    "@bull-board/nestjs": "^5.20.1",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.9",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/platform-socket.io": "^10.3.9",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/websockets": "^10.3.9",
    "@prisma/client": "^5.15.0",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "bull": "^4.12.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "graphql": "^16.8.1",
    "graphql-query-complexity": "^0.12.0",
    "graphql-type-json": "^0.3.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "log4js": "^6.9.1",
    "log4js-json-layout": "^2.2.3",
    "nestjs-i18n": "^10.4.5",
    "nodemailer": "^6.9.13",
    "otplib": "^12.0.1",
    "prisma-nestjs-graphql": "^20.0.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "ts-morph": "^22.0.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@jetbrains/eslint-config": "^5.4.2",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.2",
    "@types/nodemailer": "^6.4.15",
    "@types/passport-local": "^1.0.38",
    "@types/serve-index": "^1.9.4",
    "@types/source-map-support": "^0.5.10",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-comments": "^1.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-markdown": "^5.0.0",
    "eslint-plugin-no-secrets": "^1.0.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-security": "^3.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-unicorn": "^53.0.0",
    "globals": "^15.4.0",
    "husky": "^9.0.11",
    "jest": "29.7.0",
    "npm-check-updates": "^16.14.20",
    "prettier": "^3.3.1",
    "preview-email": "^3.0.20",
    "prisma": "^5.15.0",
    "rimraf": "^5.0.7",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.1.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.5",
    "typescript-eslint": "^7.12.0"
  }
}
